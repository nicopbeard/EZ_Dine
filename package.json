{
  "name": "EZ-Dine",
  "repository": {
    "type": "git",
    "url": "git://github.com/cse264/final-project-rational-b-rose.git"
  },
  "version": "1.0.0",
  "description": "A web application for in-restaurant ordering",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm run client-install && npm run build"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "author": "Rational (B)rose",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "@okta/oidc-middleware": "^4.0.1",
    "@okta/okta-react": "^1.2.3",
    "@okta/okta-sdk-nodejs": "^4.3.1",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "^4.16.2",
    "express-session": "^1.17.1",
    "heroku": "^7.47.3",
    "http-errors": "~1.6.3",
    "mongoose": "^5.6.0",
    "morgan": "~1.9.1",
    "node-forge": "^1.3.0",
    "react-router-dom": "^5.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1"
  }
}
